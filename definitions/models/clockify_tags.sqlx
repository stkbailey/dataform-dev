config { type: "view" }

with source as (
  select * from ${"tablename"}
),
renamed as (
  select
    -- keys
    id :: TEXT as tag_id,
    workspaceid :: TEXT as workspace_id,

    -- details
    name :: TEXT as tag_name,
    archived :: BOOLEAN as is_archived,

    -- metadata
    _sdc_batched_at::TIMESTAMP as loaded_at
  
  from source
)
select * from renamed