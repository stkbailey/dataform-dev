with source as (
  select * from ${ref("clients")}
),
renamed as (
  select
    -- keys
    id :: TEXT as client_id,
    workspaceid :: TEXT as workspace_id,

    -- details
    name :: TEXT as client_name,
    archived :: BOOLEAN as is_archived,

    -- metadata
    _sdc_batched_at::TIMESTAMP as loaded_at
  
  from source
)
select * from renamed